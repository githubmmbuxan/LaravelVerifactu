{
    "name": "mmbuxan/laravel-verifactu",
    "description": "Paquete Laravel para gestión y registro de facturación electrónica VeriFactu. Compatible con Laravel 10 a 12.x",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "verifactu", "facturacion electrónica"],
    "authors": [
        {
            "name": "Jorge Picón",
            "email": "jorge@mmbuxan.com",
            "homepage": "https://www.mmbuxan.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "MMBuxan\\VeriFactu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Factories\\MMBuxan\\VeriFactu\\Models\\": "database/factories/MMBuxan/VeriFactu/Models/"
          }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MMBuxan\\VeriFactu\\Providers\\VeriFactuServiceProvider"
            ],
            "aliases": {
                "VeriFactu": "MMBuxan\\VeriFactu\\Facades\\VeriFactu"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.5",
        "orchestra/testbench": "^8.0|^9.0|^10.4",
        "pestphp/pest": "^2.0"
    }
}
